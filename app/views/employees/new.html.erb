<h2>Create New Employee</h2>
<%= form_for (@employee) do |f| %>
    <label>Name</label>
    <%= f.text_field(:name)%><br>
    <%= f.label(:title)%>
    <%= f.text_field(:title)%><br>
    <%= f.label(:company_id)%>
    <%= f.collection_select :company_id, Company.all, :id, :name %><br>
    <%= f.label(:email)%>
    <%= f.text_field(:email)%><br>
    <%= f.label(:photo)%>
    <%= f.file_field(:photo)%><br>
    <%= f.submit %>
    <% end %>