<h1>Welcome</h1>

<%= form_with( url: '/sessions', method: :post ) do | form | %>
    <div class="form-group">
        <%= form.label "Email" %>
        <%= form.text_field :email %>
    </div>
     <div class="form-group">
        <%= form.label "Password" %>
        <%= form.password_field :password %>
    </div>
    <%= form.submit %>
<% end %>